{
  "telemetry.telemetryLevel": "off",
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.minimap.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.linkedEditing": true,
  "editor.fontFamily": "'Operator Mono Lig', 'FiraCode Nerd Font', 'Academy Engraved LET Plain', Consolas, monospace",
  "editor.fontLigatures": true,
  "editor.inlayHints.fontFamily": "'Operator Mono Lig', 'FiraCode Nerd Font', Consolas, monospace",
  "editor.fontWeight": "500",
  "editor.rulers": [
    { "column": 80, "color": "#5a5a5a30" },
    { "column": 120, "color": "#5a5a5a30" }
  ],
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "One Dark Pro",
  "workbench.colorCustomizations": {
    "editorIndentGuide.background": "#00000000",
    "editorIndentGuide.activeBackground": "#444444b9"
  },
  "window.restoreWindows": "none",
  "window.openFilesInNewWindow": "default",
  "window.zoomLevel": -1,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsicons.dontShowNewVersionMessage": true,
  "files.associations": {
    "*.toml": "yaml",
    "*.env": "ini",
    "*.css": "scss"
  },
  "rust-analyzer.inlayHints.enable": true,
  "svelte.enable-ts-plugin": true,
  "gitlens.defaultDateFormat": null,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "vim.foldfix": true,
  "vim.useCtrlKeys": false,
  "vim.handleKeys": {
    "<C-d>": false
  },
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.default.cppStandard": "c++20",
  "eslint.enable": true,
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
  "editor.lineHeight": 2.5,
}
